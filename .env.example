# Copy this to .env and fill in your values

# Groq API key from console.groq.com
GROQ_API_KEY=gsk_xxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Random secret for session encryption (generate with: python -c "import secrets; print(secrets.token_hex(32))")
SECRET_KEY=change-this-to-a-random-secret

# For local dev: http://localhost:8000/api/auth/callback
# For Railway:   https://your-app.railway.app/api/auth/callback
REDIRECT_URI=http://localhost:8000/api/auth/callback
